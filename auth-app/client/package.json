  "main": "index.js",
  "scripts": {
    "serve": "cross-env NODE_ENV=development webpack s --mode development --open --port 5000 --static-directory ./out --stats-error-details",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "build:dev": "cross-env NODE_ENV=development webpack --mode development",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest --verbose ./src",
    "test:unit:copy": "jest --verbose ./__tests__/unit",
    "test:e2e": "jest --config=jest-playwright.config.ts --verbose __tests__/e2e --passWithNoTests",
    "test:coverage": "jest --config=jest-playwright.config.ts --coverage",
    "stats": "cross-env SMP=OFF webpack --json > stats.json && webpack-bundle-analyzer stats.json out",
    "clean": "rimraf node_modules out stats.json",
    "eslint": "eslint ./src/**/*.{js,jsx,ts,tsx}",
    "eslint:fix": "eslint ./src/**/*.{js,jsx,ts,tsx} --fix",
    "eslint:pre-commit": "eslint ./src/**/*.{js,jsx,ts,tsx} --fix --color --maxwarnings=0",
    "prettier:pre-commit": "prettier --write ./src/**/*.{js,jsx,ts,tsx}",
    "stylelint": "stylelint **/*.scss",
    "stylelint:fix": "stylelint **/*.scss --fix",
    "stylelint:pre-commit": "stylelint **/*.scss --fix --color --maxwarnings=0"
